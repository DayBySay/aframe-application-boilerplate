{
    "name": "aframe-my-comp-component",
    "version": "1.0.0",
    "description": "My Comp component for A-Frame.",
    "main": "index.js",
    "scripts": {
        "build": "browserify scripts/main.js -o app/scripts/build.js",
        "dev": "budo scripts/main.js:app/scripts/build.js --dir app --port 8000 --live --open",
        "dist": "webpack index.js dist/app.js && webpack -p index.js dist/app.min.js",
        "lint": "semistandard -v | snazzy",
        "prepublish": "npm run dist",
        "preghpages": "npm run build && shx rm -rf gh-pages && shx mkdir gh-pages && shx cp -r app/* gh-pages",
        "ghpages": "npm run preghpages && ghpages -p gh-pages",
        "start": "npm run dev",
        "unboil": "node scripts/unboil.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jujunjun110/my-comp.git"
    },
    "keywords": [
        "aframe",
        "aframe-component",
        "aframe-vr",
        "vr",
        "mozvr",
        "webvr"
    ],
    "author": "jun",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jujunjun110/my-comp/issues"
    },
    "homepage": "https://github.com/jujunjun110/my-comp#readme",
    "dependencies": {
        "aframe": "^0.4.0",
        "browserify": "^13.0.0",
        "browserify-css": "^0.9.1",
        "budo": "^8.2.2",
        "ghpages": "^0.0.8",
        "inquirer": "^1.0.2",
        "randomcolor": "^0.4.4",
        "semistandard": "^8.0.0",
        "shelljs": "^0.7.0",
        "shx": "^0.1.1",
        "snazzy": "^4.0.0",
        "webpack": "^1.13.0"
    },
    "semistandard": {
        "ignore": [
            "app/build.js",
            "dist/**"
        ]
    }
}
